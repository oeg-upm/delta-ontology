                        owl:allValuesFrom :Cluster
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasProfile ;
                        owl:allValuesFrom :Profile
                      ] ;
      rdfs:comment "Set of devices" ;
      rdfs:label "FEID" .


