<?xml version="1.0"?>
<rdf:RDF xmlns="http://delta.linkeddata.es/def/core#"
     xml:base="http://delta.linkeddata.es/def/core"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:core="http://delta.linkeddata.es/def/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://delta.linkeddata.es/def/core">
        <dc:title>DELTA ontology</dc:title>
        <terms:creator rdf:resource="http://www.garcia-castro.com/foaf.rdf#me"/>
        <terms:creator>Alba Fern√°ndez Izquierdo</terms:creator>
        <terms:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
        <rdfs:comment>This ontology represents the  terms to allow interoperability in DELTA project. It models concepts related to enery market, smart grids and demand-response platforms, and it will reuse standard ontologies whenever possible.</rdfs:comment>
        <owl:versionInfo>0.1.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://delta.linkeddata.es/def/core#preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://delta.linkeddata.es/def/core#preferredNamespaceUri"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://delta.linkeddata.es/def/core#activates -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#activates">
        <rdfs:label>activates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#belongsTo -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#belongsTo">
        <owl:inverseOf rdf:resource="http://delta.linkeddata.es/def/core#owns"/>
        <rdfs:comment>Relation between a device and a FEID</rdfs:comment>
        <rdfs:label>belongs to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#estimates -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#estimates">
        <rdfs:comment>Relation between an Aggregator and the entity that it estimates</rdfs:comment>
        <rdfs:label>estimates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasAggregatedProperty -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasAggregatedProperty">
        <rdfs:comment>Aggregated property that a FEID or Virtual Node has related to the devices they own</rdfs:comment>
        <rdfs:label>has aggregated property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasCluster -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasCluster">
        <rdfs:comment>Relation between an entity and the cluster it belongs to</rdfs:comment>
        <rdfs:label>has cluster</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasCurrency -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasCurrency">
        <rdfs:comment>Link between a price and its currency</rdfs:comment>
        <rdfs:label>has currency</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasDuration -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasDuration">
        <rdfs:comment>Duration of an event</rdfs:comment>
        <rdfs:label>has duration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasIncentive -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasIncentive">
        <rdfs:comment>Link between a corrected value of energy and its incentive</rdfs:comment>
        <rdfs:label>has incentive</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasPrice -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasPrice">
        <rdfs:comment>Link between a price and an entity</rdfs:comment>
        <rdfs:label>has price</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasProfile -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasProfile">
        <rdfs:comment>Link between an entity and its profile</rdfs:comment>
        <rdfs:label>has profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasReward -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasReward">
        <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Customer"/>
        <rdfs:comment>Link between a customer and its associated reward</rdfs:comment>
        <rdfs:label>has reward</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isAllocatedIn -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isAllocatedIn">
        <rdfs:comment>Relation between a Virtual Node and the aggregator associated</rdfs:comment>
        <rdfs:label>is allocated in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isOwnedBy -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isOwnedBy">
        <rdfs:label>is owned by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isRelatedToProperty -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isRelatedToProperty">
        <rdfs:label>is related to property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#manage -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#manage">
        <rdfs:comment>Relation between a Virutal Node and the  devices it manages</rdfs:comment>
        <rdfs:label>manage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#owns -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#owns">
        <rdfs:label>owns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#send -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#send">
        <rdfs:label>send</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#sendBids -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#sendBids">
        <rdfs:label>send bids</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#uses -->


    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#uses">
        <rdfs:label>uses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isMeasuredIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isMeasuredIn"/>
    


    <!-- https://w3id.org/saref#isRelatedToMeasurement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isRelatedToMeasurement"/>
    


    <!-- https://w3id.org/saref#makesMeasurement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/saref#makesMeasurement"/>
    


    <!-- https://w3id.org/saref#measuresProperty -->


    <owl:ObjectProperty rdf:about="https://w3id.org/saref#measuresProperty"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://delta.linkeddata.es/def/core#capacitySold -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#capacitySold">
        <rdfs:label>capacity sold</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#clusterDescription -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>Description of a cluster</rdfs:comment>
        <rdfs:label>cluster description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#clusterName -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterName">
        <rdfs:comment>Name of a cluster</rdfs:comment>
        <rdfs:label>cluster name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#granularity -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#granularity">
        <rdfs:comment>granularity of the profile (e.g., weekly, daily)</rdfs:comment>
        <rdfs:label>granularity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasBeginning -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasBeginning">
        <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#TimeInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment>Beginning of a time interval</rdfs:comment>
        <rdfs:label>has beginning</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasEnd -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasEnd">
        <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#TimeInterval"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment>End of a time interval</rdfs:comment>
        <rdfs:label>has end</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasTimestamp -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasTimestamp">
        <rdfs:label>has timestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#stateOfCharge -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#stateOfCharge">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>state of charge</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#type -->


    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#type">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/saref#hasValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/saref#hasValue"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://delta.linkeddata.es/def/core#Aggregator -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Aggregator">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#estimates"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Bid"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#sendBids"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#EnergyMarket"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Legal entities that aggregate the load or generation of various demand and/or generation/production units and aim at optimizing energy supply and consumption.</rdfs:comment>
        <rdfs:label>Aggregator</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Bid -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Bid">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasDuration"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#TimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Bid that is estimated by the aggregator</rdfs:comment>
        <rdfs:label>Bid</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Cluster -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Cluster">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasAggregatedProperty"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/saref#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Aggrupation of elements, such as FEIDs or Virtual Nodes</rdfs:comment>
        <rdfs:label>Cluster</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Consumer -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Consumer">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Prosumer"/>
        <rdfs:comment>Entity that consumes energy</rdfs:comment>
        <rdfs:label>Consumer</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Customer -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Customer">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#owns"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Device"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasReward"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Reward"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#type"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#rational"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Entity that interacts in the platform</rdfs:comment>
        <rdfs:label>Customer</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Device -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Device">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#belongsTo"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#FEID"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#isOwnedBy"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Customer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#stateOfCharge"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#rational"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Device that is located in a specific place</rdfs:comment>
        <rdfs:label>Device</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Energy -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Energy">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Property"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasIncentive"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Incentive"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A property related to measurements that are characterized  by a value measured in energy units</rdfs:comment>
        <rdfs:label>Energy</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#EnergyMarket -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#EnergyMarket"/>
    


    <!-- http://delta.linkeddata.es/def/core#FEID -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#FEID">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasAggregatedProperty"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/saref#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasCluster"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Cluster"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Set of devices</rdfs:comment>
        <rdfs:label>FEID</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Flexibility -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Flexibility">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Property"/>
        <rdfs:comment>Flexibility is a property that indicates the ability to increase or reduce the production of power plants or the consumption of demand processes</rdfs:comment>
        <rdfs:label>Flexibility</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#FlexibilityBid -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#FlexibilityBid">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Bid"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#capacitySold"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Bid related to energy  flexibility</rdfs:comment>
        <rdfs:label>Flexibility bid</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Generation -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Generation">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Property"/>
        <rdfs:comment>A property related to measurements that are characterized  by a value measured in generation units. It represents the energy produced.</rdfs:comment>
        <rdfs:label>Generation</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Incentive -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Incentive">
        <rdfs:comment>Incentive (usually monetary) associated to energy values</rdfs:comment>
        <rdfs:label>Incentive</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Load -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Load">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Property"/>
        <rdfs:comment>A property related to measurements that are characterized  by a value measured in energy units. It represents the load needed for a device to init</rdfs:comment>
        <rdfs:label>Load</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Power -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Power">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Property"/>
        <rdfs:comment>A property related to measurements that are characterized  by a value measured in power units</rdfs:comment>
        <rdfs:label>Power</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#PriceBid -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#PriceBid">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Bid"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Price"/>
        <rdfs:comment>Bid related to price</rdfs:comment>
        <rdfs:label>Price bid</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#PriceBidAS -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#PriceBidAS">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#PriceBid"/>
        <rdfs:comment>Price bid for ancillary services</rdfs:comment>
        <rdfs:label>Price bid AS</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#PriceBidDayAhead -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#PriceBidDayAhead">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#PriceBid"/>
        <rdfs:comment>Bid price for the next day</rdfs:comment>
        <rdfs:label>Price bid day ahead</rdfs:label>
    </owl:Class>
    


